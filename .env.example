APP_NAME=Rotor
APP_ENV=production
APP_KEY=00000000000000000000000000000000
APP_DEBUG=true
APP_URL=http://localhost
APP_EMAIL=admin@localhost
APP_ADMIN=admin
APP_PORT=8080

