{
    "name": "visavi/rotor",
    "type": "project",
    "description": "Rotor",
    "keywords": ["php cms", "wap cms", "mobile cms"],
    "homepage": "https://visavi.net",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Alexander Grigorev",
            "email": "admin@visavi.net",
            "homepage": "https://visavi.net"
        },
        {
            "name": "Rotor Community",
            "homepage": "https://github.com/visavi/rotor/graphs/contributors"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "cbschuld/browser.php": "~1.9",
        "google/recaptcha": "~1.3",
        "gregwar/captcha": "^1.2",
        "guzzlehttp/guzzle": "^7.9",
        "intervention/image": "^3.11",
        "laravel/framework": "^12.0",
        "php-ffmpeg/php-ffmpeg": "^1.3",
        "visavi/captcha": "^3.0",
        "visavi/cleanup": "^1.5"
    },
    "require-dev": {
        "deployer/deployer": "^7.5",
        "fakerphp/faker": "^1.24",
        "laravel/pint": "^1.22",
        "laravel/sail": "^1.41",
        "laravel/tinker": "^2.10",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules/"
        },
        "files": [
            "app/helpers.php",
            "app/hooks.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "phpstan analyse --no-progress -vvv",
        "test": "vendor/bin/phpunit",
        "lint": "vendor/bin/pint --test",
        "fix": "vendor/bin/pint",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "12.4-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform": {
            "php": "8.2.0"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
