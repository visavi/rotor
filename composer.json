{
    "name": "visavi/rotor",
    "type": "project",
    "description": "Rotor",
    "keywords": ["php cms", "wap cms", "mobile cms"],
    "homepage": "https://visavi.net",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Alexander Grigorev",
            "email": "admin@visavi.net",
            "homepage": "https://visavi.net"
        },
        {
            "name": "Rotor Community",
            "homepage": "https://github.com/visavi/rotor/graphs/contributors"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "cbschuld/browser.php": "~1.9",
        "fruitcake/laravel-cors": "^2.2",
        "google/recaptcha": "~1.2",
        "gregwar/captcha": "^1.1",
        "guzzlehttp/guzzle": "^7.4",
        "intervention/image": "^2.7",
        "laravel/framework": "^9.5",
        "nelexa/zip": "^4.0",
        "php-ffmpeg/php-ffmpeg": "^1.0",
        "visavi/captcha": "^3.0",
        "visavi/cleanup": "^1.5"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.19",
        "laravel/sail": "^1.13",
        "laravel/tinker": "^2.7",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.4",
        "php-coveralls/php-coveralls": "^2.5",
        "slevomat/coding-standard": "^7.0",
        "squizlabs/php_codesniffer": "^3.6",
        "deployer/deployer": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "phpstan": "phpstan analyse --no-progress -vvv",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.0-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
